[
    {
        "type": "about",
        "description": [
            "Creates a chain of directories."
        ]
    },
    {
        "type": "include",
        "paths": [
            "./src/libs"
        ]
    },
    {
        "type": "folder",
        "name": "~/bin"
    },
    {
        "type": "folder",
        "name": "~/src"
    },
    {
        "type": "folder",
        "name": "~/src/programs"
    },
    {
        "type": "folder",
        "name": "~/src/programs/shell-extensions"
    },
    {
        "type": "file",
        "local": "./src/programs/shell-extensions/mkdir-p.gs",
        "file": "~/src/programs/shell-extensions/mkdir-p.gs"
    },
    {
        "type": "build",
        "source": "~/src/programs/shell-extensions/mkdir-p.gs",
        "target": "~/bin/mkdir-p"
    }
]
