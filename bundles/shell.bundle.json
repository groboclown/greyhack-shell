[
    {
        "type": "about",
        "description": "A fancy shell replacement.  Kind of like Midnight Commander.",
        "notes": [
            "Order of files is important."
        ]
    },
    {
        "type": "compile",
        "local": "../src/cmdlets/hello.gs",
        "target": "~/bin/cmdlets/hello"
    },
    {
        "type": "compile",
        "local": "../src/cmdlets/pagecontroller.gs",
        "target": "~/bin/cmdlets/pagecontroller"
    },
    {
        "type": "compile",
        "local": "../src/programs/mc/main.gs",
        "target": "~/bin/mc",
        "local-tests": [
            "../src/libs/context/*-test.gs",
            "../src/libs/format/*-test.gs",
            "../src/libs/files/*-test.gs",
            "../src/libs/std-lib/*-test.gs",
            "../src/programs/mc/*-test.gs"
        ]
    }
]
